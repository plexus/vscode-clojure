{
  "name": "clojure",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.0.0",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "^1.52.0"
  },
  "scripts": {
    "update-grammar": "node ../../build/npm/update-grammar.js atom/language-clojure grammars/clojure.cson ./syntaxes/clojure.tmLanguage.json",
    "vscode:prepublish": "npm run test",
    "test": "node ./test/runTest.js"
  },
  "contributes": {
    "languages": [
      {
        "id": "clojure",
        "aliases": [
          "Clojure",
          "clojure"
        ],
        "extensions": [
          ".clj",
          ".cljs",
          ".cljc",
          ".cljx",
          ".clojure",
          ".edn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "clojure",
        "scopeName": "source.clojure",
        "path": "./syntaxes/clojure.tmLanguage.json"
      }
    ]
  },
  "homepage": "https://code.visualstudio.com/",
  "bugs": {
    "url": "https://github.com/microsoft/vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-clojure.git"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/vscode": "^1.52.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "vscode-test": "^1.4.1"
  }
}
